####### DIMENSIONLESS PARAMETERS #######

# Reynolds-Number -> corresponds with tau, default [10]
Reynolds = 40

# Morton-Number, default [100]
Morton = 1e-4

# Eotvos-Number, default [10]
Eotvos = 3

#### DOMAIM SIZE ####

# resolution / Cells per Bubble diameter, default [30]
resolution = 20

# number of Cells in x direction, default [50]
xCells = 60

# number of Cells in y direction,, default [50]
yCells = 60

# number of Cells in z direction,, default [50]
zCells = 60

####### DENSITY #######

# density of the denser phase / kg m^-3, default [1]
rho_l = 1

# density ratio, default [2]
gamma = 2

####### PARAMETERS FOR RELAXATION-MATRIX #######

# bulk viscosity ratio mu'/mu  , default [2]
mu_ratio = 1.8

# s_3 , default [1]
s_3 = 1.2

# s_5 , default [1]
s_5 = 1.2

# s_11 , default [1]
s_11 = 1.2

# s_17 , default [1]
s_17 = 1.2 

####### SHEAR FLOW #######

# does simulation feature shear flow, default [false]
isShearFlow = false

# shear rate in 1/s, default [0]
shearRate = 0

####### CONTOLS #######

# maximum number of time steps, default [1e5]
max_steps = 1e3

# output Settings
output_interval = 50
restart_interval = 1000

